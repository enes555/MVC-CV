@using MvcCv.Models.Entity;
@model List<TBLEducation>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/About/Index.cshtml";
}

<h2>Eğitimler</h2>


<table class="table table-bordered">

    <tr>

        <th> ID</th>
        <th> Title</th>
        <th> Subtitle</th>
        <th> Subtitle2</th>
        <th> GNO</th>
        <th>Delete</th>
        <th>Update</th>
    </tr>
    @foreach (var cat in Model)
    {


        <tr>
            <td>@cat.ID</td>
            <td>@cat.Title</td>
            <td>@cat.subtitle</td>
            <td>@cat.subtitle2</td>
            <td>@cat.GNO</td>
            <td><a href="/Education/EğitimGetir/@cat.ID" class="btn btn-success">Update</a></td>
            <td><a href="/Education/DeleteEducation/@cat.ID" class="btn btn-danger">Delete</a></td>


        </tr>
    }

</table>
<a href="/Education/AddEducation/" class="btn btn-primary">Add Education </a>
